{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install telethon==1.34.0",
    "nixpacksConfig": {
      "assets": {
        "genesis_session.session": "./genesis_session.session"
      }
    }
  },
  "volumes": [
    {
      "name": "session-storage",
      "mountPath": "/app/genesis_session.session",
      "source": "local-file"
    }
  ]
}
